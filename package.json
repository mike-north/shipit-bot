{
  "name": "shipit-bot",
  "version": "1.0.0",
  "description": "ACL bot",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "license": "BSD-2-Clause",
  "repository": "https://github.com//shipit.git",
  "homepage": "https://github.com//shipit",
  "bugs": "https://github.com//shipit/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "dev:watch": "nodemon --exec \"npm start\"",
    "watch": "yarn concurrently \"npm:build:watch\" \"npm:dev:watch\" -n \"TS,Run\" -c \"bgYellow.black,bgGreen.black\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.7",
    "@types/nock": "^9.3.0",
    "debug": "^4.1.1",
    "js-yaml": "^3.13.1",
    "nock": "^10.0.0",
    "probot": "^9.3.2"
  },
  "devDependencies": {
    "@octokit/graphql-schema": "^6.31.1",
    "@playlyfe/gql": "^2.6.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.0",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^11.9.0",
    "concurrently": "^4.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "nodemon": "^1.17.2",
    "smee": "^0.2.0",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
