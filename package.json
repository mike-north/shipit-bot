{
  "name": "shipit-bot",
  "version": "0.0.0",
  "description": "ACL bot",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "license": "BSD-2-Clause",
  "repository": "https://github.com/mike-north/shipit-bot.git",
  "homepage": "https://github.com/mike-north/shipit-bot",
  "bugs": "https://github.com/mike-north/shipit-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "private": true,
  "scripts": {
    "clean": "rimraf lib test-js",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "dev:watch": "yarn pm2 start ./lib/index.js --watch",
    "watch": "yarn concurrently \"npm:build:watch\" \"npm:dev:watch\" \"yarn pm2 log\" -n \"TS,PM,Log\" -c \"bgYellow.black,bgGreen.black,bgWhite.black\"",
    "start": "yarn pm2 start ./lib/index.js -i max",
    "lint": "yarn eslint src test --ext js,ts",
    "pretest": "yarn tsc -b test",
    "test": "yarn qunit test-js/test/**/*.test.js",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.7",
    "debug": "^4.1.1",
    "js-yaml": "^3.13.1",
    "pm2": "^3.5.1",
    "probot": "^9.3.2"
  },
  "devDependencies": {
    "@octokit/graphql-schema": "^6.31.1",
    "@playlyfe/gql": "^2.6.2",
    "@types/debug": "^4.1.5",
    "@types/js-yaml": "^3.12.1",
    "@types/nock": "^9.3.0",
    "@types/node": "^11.9.0",
    "@types/qunit": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "concurrently": "^4.1.2",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-typescript": "^0.14.0",
    "nock": "^10.0.0",
    "qunit": "^2.9.2",
    "rimraf": "^3.0.0",
    "smee": "^0.2.0",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "volta": {
    "node": "12.9.1",
    "yarn": "1.17.3"
  }
}
